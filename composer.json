{
    "name": "evanshunt/lazy-focus-fit",
    "description": "Template methods to quickly make use of FocusPoint, LazySizes, and Object-fit",
    "type": "silverstripe-vendormodule",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "EvansHunt\\LazyFocusFit\\": "src/"
        }
    },
    "authors": [
        {
            "name": "David Toews",
            "email": "davejtoews@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lerni/silverstripe-focuspoint"
        }
    ],
    "require": {
        "jonom/focuspoint": "dev-getHeight-getWidth as 4.0.3",
        "silverstripe/framework": "^4",
        "silverstripe/cms": "^4"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
